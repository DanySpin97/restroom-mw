    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "bugs": {
    "url": "https://github.com/dyne/restroom-wm/issues"
  },
  "dependencies": {
    "@restroom-mw/zencode": "^0.4.0",
    "capture-console": "^1.0.1",
    "zenroom": "^1.1.3"
  },
